<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.unscripted.www.delocker.DeLock">

    <TextView
        android:id="@+id/txtVwTHREE"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="TextView"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:layout_alignTop="@+id/txtVwTWO"
        android:layout_toRightOf="@+id/txtVwTWO"
        android:layout_toEndOf="@+id/txtVwTWO" />

    <TextView
        android:id="@+id/txtVwTWO"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/border"
        android:text="TextView"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/txtViewONE"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:text="TextView"
        android:layout_alignBaseline="@+id/txtVwTHREE"
        android:layout_alignTop="@+id/txtVwTWO"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp" />
</RelativeLayout>
